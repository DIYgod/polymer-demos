  "bugs": {
    "url": "https://github.com/DIYgod/polymer-demo/issues"
  },
  "homepage": "https://github.com/DIYgod/polymer-demo#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "webpack": "^4.1.1",
    "webpack-cli": "2.1.4",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@polymer/iron-input": "3.0.0-pre.19",
